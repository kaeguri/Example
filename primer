#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL,"Russian");
    
    //Создадим массив
    const int n = 5;
    int mas [n];
    
    //Заполним массив "рандомными" числами от 0 до 99 и выведем на экран каждый элемент
    for (int i = 0; i < n; i++) {
        mas[i] = rand() % 100;
        cout << i << " элемент массива: " << mas[i] << endl;
    }
    
    return 0;
}
